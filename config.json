{
	"vhost": "http://gulp:80",
	"dist": "public/assets/",
	"sassIndex": "source/styles/index.scss",
	"sassFilesToLint": [
		"source/styles/**"
	],
	"typescriptsToLint": [
		"source/scripts/*.ts",
		"source/scripts/**/*.ts",
		"!source/scripts/typings/*.ts",
		"!source/scripts/typings/**/*.ts"
	],
	"scriptEntries": [
		"source/scripts/index.ts"
	],
	"watchedViews": [
		"public/*.html"
	]
}