{
	"vhost": "http://gulp:80",
	"dist": "public/assets/",
	"scripts": [
		"source/scripts/*.js",
		"source/scripts/**/*.js",
		"source/scripts/**/**/*.js",
		"!source/scripts/inline/**"
	],
	"scriptsInline": [
		"source/scripts/inline/*.js",
		"source/scripts/inline/**/*.js",
		"source/scripts/inline/**/**/*.js"
	],
	"scriptsToLint": [
		"source/scripts/*.js",
		"source/scripts/**/*.js",
		"source/scripts/**/**/*.js",
		"source/scripts/**/**/**/*.js",
		"!source/libs/**"
	],
	"sassIndex": [
		"source/styles/index.scss",
		"source/styles/inline.scss",
		"source/styles/wp-editor-styles.scss"
	],
	"sassFilesToLint": [
		"source/styles/**",
		"!source/styles/libs/**"
	],
	"watchedViews": [
		"public/*.html"
	]
}